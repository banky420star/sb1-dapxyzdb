{
  "name": "algo-trading-system",
  "version": "1.0.0",
  "description": "Self-driving trading stack with ML ensemble and risk management",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"vite\"",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node server/index.js",
    "start": "pm2-runtime ecosystem.config.cjs",
    "start:dev": "node server/index.js",
    "start:simple": "node start-simple.js",
    "stop": "pm2 stop ecosystem.config.cjs",
    "restart": "pm2 restart ecosystem.config.cjs",
    "logs": "pm2 logs",
    "test": "vitest",
    "test:e2e": "vitest run tests/e2e",
    "test:integration": "vitest run tests/integration",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "train": "node server/ml/train.js",
    "backtest": "node server/trading/backtest.js",
    "data:fetch": "node server/data/fetcher.js",
    "models:validate": "node server/ml/validator.js",
    "setup:mt5": "node start_mt5_trading.js",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "db:reset": "node scripts/reset-db.js",
    "prefect:deploy": "python flows/deploy.py",
    "prefect:run": "python flows/run_pipeline.py",
    "docker:build": "docker build -t ai-trading-system .",
    "docker:up": "docker compose up --build",
    "docker:down": "docker compose down"
  },
